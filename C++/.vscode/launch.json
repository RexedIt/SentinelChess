{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) SentinelConsole (Debug)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Console/Debug/SentinelConsole.exe",
            "args": [
                "--chessdata",
                "../ChessData"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "preLaunchTask": "build debug" // corresponds to a task in tasks.json that builds your program
        },
